{
  "name": "react-native-tuikit-atomic-x",
  "version": "0.0.1-beta",
  "description": "Tuikit-Atomic-X SDK is a reactive API SDK launched by Tencent Cloud for scenarios such as instant messaging, audio/video calls, live video streaming, and voice chat rooms. It supports features including room management, broadcaster go-live, viewer watching, member management, microphone position control, and basic beautification.broadcaster go-live, viewer watching, member management, microphone position control, and basic beautification.",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-tuikit-atomic-x-example",
    "clean": "del-cli android/build lib",
    "prepare": "bob build",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "jest",
    "release": "release-it --only-version"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://trtc.io.git"
  },
  "author": "mileszzhang <mileszzhang@tencent.com> (https://trtc.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://trtc.io/issues"
  },
  "homepage": "https://trtc.io#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.0",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "lefthook": "^2.0.3",
    "prettier": "^2.8.8",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-builder-bob": "^0.40.13",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-toast-message": "^2.3.3",
    "release-it": "^19.0.4",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-localize": "^3.0.0",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-toast-message": "^2.0.0"
  },
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "TuikitAtomicXSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "io.trtc.tuikit.atomicxcore.hybridapi"
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "create-react-native-library": {
    "type": "turbo-module",
    "languages": "kotlin-objc",
    "tools": [
      "eslint",
      "jest",
      "lefthook",
      "release-it"
    ],
    "version": "0.56.0"
  },
  "dependencies": {
    "i18next": "^25.7.3",
    "intl-pluralrules": "^2.0.1",
    "react-i18next": "^16.5.0",
    "react-native-localize": "^3.6.0"
  }
}
